{
  "name": "node-retro",
  "description": "Load libretro cores within Javascript.",
  "version": "0.6.5",
  "author": "Matthew Bauer <mjbauer95@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewbauer/node-retro.git"
  },
  "main": "retro.js",
  "scripts": {
    "prepublish": "gulp prepublish",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "gulp test"
  },
  "dependencies": {
    "nan": "^1.7.0",
    "node-arraybuffer": "^1.0.7",
    "node-pre-gyp": "^0.6.4",
    "request": "^2.55.0",
    "unzip": "^0.1.11"
  },
  "binary": {
    "module_name": "retro",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "host": "https://node-retro.s3-website-us-west-2.amazonaws.com",
    "remote_path": "./{name}/v{version}"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "aws-sdk": "^2.1.29",
    "coffee-script": "^1.9.2",
    "gametime-nointro": "^0.5.1",
    "gulp": "^3.8.11",
    "gulp-coffee": "^2.3.1",
    "gulp-mocha": "^2.0.1",
    "gulp-shell": "^0.4.1",
    "gulp-util": "^3.0.4",
    "should": "^6.0.1"
  },
  "keywords": [
    "emulation",
    "libretro",
    "games",
    "bindings"
  ]
}
